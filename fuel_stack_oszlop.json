{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 640,
  "height": 360,
  "title": "Meghajtás",
  "data": {
    "url": "https://raw.githubusercontent.com/HobajHunor/hobajhunor.github.io/main/germ130.csv"
  },
  "mark": {"type": "bar", "cornerRadiusTopLeft": 4, "cornerRadiusTopRight": 3},
  "encoding": {
    "x": {
      "timeUnit": "year",
      "field": "year",
      "type": "ordinal",
      "title": "Évek"
    },
    "y": {
      "aggregate": "count",
      "type": "quantitative",
      "title": "Eladások száma (darab)"
    },
    "color": {"field": "fuel", "type": "nominal", "title": "Üzemanyag típusok"}
  },
  "config": {}
}