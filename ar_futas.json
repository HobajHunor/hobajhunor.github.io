{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 640,
  "height": 360,
  "title": "Az ár és a futásteljesítmény összefüggése",
  "data": {
    "url": "https://raw.githubusercontent.com/HobajHunor/hobajhunor.github.io/main/germany155.csv"
  },
  "params": [{"name": "Márkák", "select": "interval", "bind": "scales"}],
  "mark": "circle",
  "encoding": {
    "x": {
      "field": "mileage",
      "type": "quantitative",
      "title": "Kilométer óra állása",
      "scale": {"domain": [0, 700000]}
    },
    "y": {
      "field": "price",
      "type": "quantitative",
      "title": "Eladási ár",
      "scale": {"domain": [0, 750000]}
    },
    "color": {"field": "legend", "type": "nominal"}
  },
  "config": {}
}